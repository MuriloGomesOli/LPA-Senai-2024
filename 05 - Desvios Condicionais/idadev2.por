*/


programa
{
	
	funcao inicio()
	{
		inteiro idade

		escreva("Digite a sua idade: \n	")
		leia(idade)

			se (idade<18)
			{
				escreva("Você é um ménó")
			}

			senao 
			{
				escreva("Você é de maió")
			}

		

		



	
	}
}

